    }
    const _initializeAuth = () => {
      if (_authService._authService.isAuthenticated()) {
        const _storedUser = _authService._authService.getUser();
        if (_storedUser) {
          setUser(_storedUser);
          setIsAdmin(_authService._authService.isAdmin());
